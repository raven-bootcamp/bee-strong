<div class="col-sm-6 col-lg-4 col-xl-3 pb-3 d-flex">
    <div class="card mx-auto" style="width: 18rem">

        <iframe src="{{courseLink}}" frameborder="0" allow="accelerometer; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
        <div class="card-body">
            <div class="d-flex flex-wrap">
                <h5 class="card-title">{{CourseTitle}}</h5>
                <div class="form-check form-switch">
                    <input class="form-check-input" type="checkbox" id="flexSwitchCheckChecked" checked>
                    <label class="form-check-label" for="flexSwitchCheckChecked">Enabled</label>
                </div>
                <div>
                    {{> tags}}
                </div>
                <a href="#" class="btn btn-warning mt-2">Update</a>
                <a href="#" class="btn btn-danger mt-2">Delete</a>
                <a href="#" class="btn btn-success mt-2">Current Students</a>
            </div>
        </div>
    </div>
</div>